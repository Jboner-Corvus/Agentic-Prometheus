{
  "name": "agentic-forge-server",
  "version": "1.0.0",
  "description": "A fully autonomous, self-improving agentic system to forge new tools, based on the FastMCP framework.",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "start:web": "node dist/webServer.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "dev": "pnpm run build && pnpm exec concurrently \"pnpm run start\" \"pnpm run start:worker\" \"pnpm run start:web\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "author": "Agentic Forge",
  "license": "MIT",
  "dependencies": {
    "bullmq": "^5.7.8",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fastmcp": "^3.5.0",
    "ioredis": "^5.4.1",
    "pino": "^9.1.0",
    "playwright": "^1.44.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/dockerode": "^3.3.41",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.4.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5",
    "typescript-eslint": "^7.8.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0"
  }
}
