{
  "name": "agentic-forge-server",
  "version": "1.0.0",
  "description": "A fully autonomous, self-improving agentic system to forge new tools, based on the FastMCP framework.",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/server.js",
    "start:worker": "node -r tsconfig-paths/register dist/worker.js",
    "start:web": "node -r tsconfig-paths/register dist/webServer.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "dev": "pnpm run build && pnpm exec concurrently \"pnpm run start\" \"pnpm run start:worker\" \"pnpm run start:web\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "clean": "rm -rf dist",
    "prepare": "husky"
  },
  "author": "Senior Refactoring AI",
  "license": "MIT",
  "dependencies": {
    "@types/dockerode": "^3.3.41",
    "@types/node": "^24.0.3",
    "bullmq": "^5.55.0",
    "dockerode": "^4.0.7",
    "dotenv": "^16.5.0",
    "fastmcp": "^3.5.0",
    "ioredis": "^5.6.1",
    "pino": "^9.7.0",
    "playwright": "^1.53.1",
    "typescript": "^5.8.3",
    "zod": "^3.25.67",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "lint-staged": "^16.1.2",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.34.1"
  },
  "engines": {
    "node": ">=24.0.2",
    "npm": ">=11.3.0"
  },
  "homepage": "YOUR_PROJECT_HOMEPAGE_HERE",
  "pnpm": {
    "overrides": {
      "glob": "^10.4.2",
      "inflight": "npm:semver@latest"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  }
}