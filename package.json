{
  "name": "agentic-prometheus-server",
  "version": "1.0.0",
  "description": "A fully autonomous, self-improving agentic system based on the FastMCP framework.",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "dev": "pnpm run build && (pnpm run start & pnpm run start:worker)",
    "dev:watch": "tsc -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "author": "Senior Refactoring AI",
  "license": "MIT",
  "dependencies": {
    "bullmq": "^5.10.2",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "fastmcp": "^3.4.0",
    "ioredis": "^5.4.1",
    "pino": "^9.2.0",
    "playwright": "^1.45.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.29",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
