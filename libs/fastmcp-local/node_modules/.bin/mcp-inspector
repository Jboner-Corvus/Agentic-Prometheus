#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/cli/build/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/cli/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules/@modelcontextprotocol/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/cli/build/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/cli/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules/@modelcontextprotocol/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector@0.11.0_@types+node@22.15.21_typescript@5.8.3/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@modelcontextprotocol/inspector/cli/build/cli.js" "$@"
else
  exec node  "$basedir/../@modelcontextprotocol/inspector/cli/build/cli.js" "$@"
fi
