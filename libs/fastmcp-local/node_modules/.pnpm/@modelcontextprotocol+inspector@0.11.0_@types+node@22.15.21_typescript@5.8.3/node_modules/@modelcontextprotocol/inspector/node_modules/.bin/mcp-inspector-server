#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector-server@0.11.0/node_modules/@modelcontextprotocol/inspector-server/build/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector-server@0.11.0/node_modules/@modelcontextprotocol/inspector-server/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector-server@0.11.0/node_modules/@modelcontextprotocol/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector-server@0.11.0/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector-server@0.11.0/node_modules/@modelcontextprotocol/inspector-server/build/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector-server@0.11.0/node_modules/@modelcontextprotocol/inspector-server/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector-server@0.11.0/node_modules/@modelcontextprotocol/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/@modelcontextprotocol+inspector-server@0.11.0/node_modules:/home/allah/Desktop/Serveur MCP Asynchrone/MCP++/libs/fastmcp-local/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@modelcontextprotocol+inspector-server@0.11.0/node_modules/@modelcontextprotocol/inspector-server/build/index.js" "$@"
else
  exec node  "$basedir/../../../../../../@modelcontextprotocol+inspector-server@0.11.0/node_modules/@modelcontextprotocol/inspector-server/build/index.js" "$@"
fi
